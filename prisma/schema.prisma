generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Prompt {
  id        String   @id @default(uuid())
  content   String   @db.Text
  resIa     String   @db.Text
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
